{
    "exposed" : true,  
    "authentication" :                                            
    {
       "method": "Basic"   
    },
    "cache_control" : "must-revalidate", 
    "cors" :                      
    {
     "enabled" : true,
     "allowMethods": ["GET","POST","HEAD","OPTIONS"], 
     "allowOrigin": ["*"]
    },          
     "enable_etags" : false,
    "xhrFields": {"withCredentials": true},
     "force_ssl" : false,
     "prevent_xsrf" : false
}
